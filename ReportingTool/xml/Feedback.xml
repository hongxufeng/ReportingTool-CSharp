  <table id="题干"
           name="(select t_imsTopic.obj_id,t_imsTopic.name,T_imspapertopiclink.roleA_id from t_imsTopic,T_imspapertopiclink where T_imspapertopiclink.roleB_id= t_imsTopic.obj_id)AS t_imsTopic">
